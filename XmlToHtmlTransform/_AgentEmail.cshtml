                                                        &nbsp;
                                                    </td>
                                                    <td id="location" style="width: 150px;">
                                                        @if (!string.IsNullOrWhiteSpace(childLN.CompeteUrl))
                                                        {
                                                            <table>
                                                                <tr>
                                                                    <td>
                                                                        <img src="@(Model.ImageBaseUrl)/abarLocation.png" width="15" />
                                                                    </td>
                                                                    <td style="text-align: center;">
                                                                        <a href="@childLN.Url" style="color: #73797F; white-space: nowrap;">@childLN.CompeteUrl</a>
                                                                    </td>
                                                                </tr>
                                                            </table>
                                                        }
                                                    </td>
                                                    <td id="sentiment" style="width: 100px;" style="text-align: center; white-space: nowrap;">
                                                        @childLN.MediaDate.ToString("MM/dd/yyyy hh:mm tt")
                                                    </td>
                                                    <td id="detail" style="width: 180px;">
                                                        @if (Decimal.Compare(Convert.ToDecimal(childLN.MediaValue), -1M) != 0 && childLN.Audience != -1)
                                                        {
                                                            <table cellspacing="0" cellpadding="0" style="height: 15px; text-align: center;">
                                                                <tr>
                                                                    <td>
                                                                        <img src="@(Model.ImageBaseUrl)/abarLocalAudience.png" alt="audience" width="15" />
                                                                    </td>
                                                                    <td style="padding-left: 10px; padding-right: 15px; font-family: 'Microsoft Sans Serif';
                                                                        text-align: center;">
                                                                        <table>
                                                                            <tr>
                                                                                <td style="text-align: center; white-space: nowrap;">
                                                                                    @if (childLN.Audience.HasValue)
                                                                                    {
                                                                                        @(string.Format("{0:n0}", childLN.Audience))
															   
                                                                                    }
                                                                                    else
                                                                                    {
                                                                                        @("NA")
                                                                                    }
                                                                                </td>
                                                                                <td style="text-align: center;">
                                                                                    @if (!string.IsNullOrWhiteSpace(childLN.Compete_Result) && childLN.Compete_Result.ToUpper() == "A")
                                                                                    {
                                                                                        <img title="Powered by Compete" width="15" height="auto" style="float: left; width: 15px; height: auto;" src="@(Model.ImageBaseUrl)/compete.png" alt="Compete Image" />
                                                                                    }
                                                                                </td>
                                                                            </tr>
                                                                        </table>
                                                                    </td>
                                                                    <td>
                                                                        <img src="@(Model.ImageBaseUrl)/dollar.png" width="15" alt="media value" />
                                                                    </td>
                                                                    <td style="padding-left: 10px; padding-right: 15px; font-family: 'Microsoft Sans Serif';
                                                                        text-align: center; white-space: nowrap;">
                                                                        @if (childLN.MediaValue.HasValue)
                                                                        {
                                                                            @(string.Format("{0:n2}", childLN.MediaValue))
                                                                        }
                                                                        else
                                                                        {
                                                                            @("NA")
                                                                        }
                                                                    </td>
                                                                </tr>
                                                            </table>
                                                        }
                                                    </td>
                                                    <td id="detail" style="width: 180px;">
                                                    </td>
                                                </tr>
                                            }
                                        </table>
                                    </td>
                                </tr>
                            }
                        </table>
                    </td>
                </tr>

                        }
                    }
                }
            }
        </table>
    }
</body>
</html>
